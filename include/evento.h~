#ifndef _EVENTO_H_
#define _EVENTO_H_
#include <string>
#include <vector>


class evento{
  int anio;
  vector<string> sucesos;

  public:

  evento();

  evento(const evento& e);

  ~evento();

  &evento operator=();

  int getanio(const evento& e);

  const vector& getsucesos(const evento& e); // ?

  string getsuceso(const evento& e, int n);

  void aniodesuceso(string s);

  void eliminasuceso(int n);

};

#endif
